\titlecontents{task}
              [3.8em] % ie, 1.5em (chapter) + 2.3em
              {}
              {\contentslabel{2.3em}}
              {\hspace*{-2.3em}}
              {\titlerule*[1pc]{.}\contentspage}

\newcounter{subtask}
\titleclass{\subtask}{straight}[\subsection]
\titleformat{\subtask}[hang]
  {}{Subtask \thetask.\thesubtask:}{1em}{}
